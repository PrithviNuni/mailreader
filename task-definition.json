{
  "family": "mailreader-task",
  "executionRoleArn": "<executionRoleArn>",
  "taskRoleArn": "<taskRoleArn>",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "mailreader",
      "image": "<IMAGE_URI>",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
